
reverse print elements 

#include <stdio.h>
void main()
{
int arr[5]={1,2,3,4,5};
int i;
for(i=4;i>-1;i--)
{
printf(" numbers are %d \n",arr[i]);
}

}
















copy elements 

#include <stdio.h>
void main()
{
int arr[5]={1,2,3,4,5};
int arr1[5];
int i;
for(i=0;i<5;i++)
{
arr1[i]=arr[i];
printf("numbers of  second array arr1[]  are ... %d \n",arr1[i]);
}
}














duplicate numbers 

#include <stdio.h>
void main()
{
int arr[5]={2,2,3,3,5};
int i;
int y =0;
for(i=0;i<5;i++)
{
int o;
for(o=i+1;o<5;o++)
{
if(arr[i]==arr[o])
{
y++;
printf("same number found \n");
printf("Total duplicates are %d \n",y);
}
}
}



unique number    ------   (let to remover errors)

#include <stdio.h>
void main()
{
int arr[5]={1,2,3,4,5};
int i;
for(i=0;i<5;i++)
{
int o;
for(o=i+1;o<5;o++)
{
if(i=!o)
{
printf("unique found \n");
}
else
{
printf("not found");
}
}




